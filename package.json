{
	"name": "devacademy",
	"version": "0.0.1",
	"description": "DevAcademy App",
	"homepage": "https://devacademy.com.au",
	"bugs": {
		"url": "https://github.com/renansigolo/devacademy/issues",
		"email": "renan.sigolo@gmail.com"
	},
	"license": "Apache-2.0",
	"author": "Renan Sigolo <renan.sigolo@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/renansigolo/devacademy.git"
	},
	"scripts": {
		"preinstall": "rm -rf node_modules package-lock.json dist",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && firebase deploy && git checkout development && git merge origin/master && git push",
		"build:prod": "ng build --prod",
		"build": "ng build",
		"clean": "rm -rf node_modules package-lock.json dist",
		"clean:install": "npm run clean && npm i",
		"lint:fix": "ng lint --fix && stylelint --fix",
		"lint:ts": "ng lint --format stylish",
		"lint:scss": "stylelint \"src/**/*.scss\"",
		"lint": "ng lint devacademy --format stylish",
		"prettier": "prettier --config ./.prettierrc.json --write \"./**/*.{js,ts,json,md,scss,component.html}\"",
		"start": "ng serve -o",
		"deploy": "ng build --prod && firebase deploy"
	},
	"dependencies": {
		"@angular/animations": "^7.2.7",
		"@angular/cdk": "^7.3.3",
		"@angular/common": "^7.2.7",
		"@angular/compiler": "^7.2.7",
		"@angular/core": "^7.2.7",
		"@angular/flex-layout": "^7.0.0-beta.23",
		"@angular/forms": "^7.2.7",
		"@angular/http": "^7.2.7",
		"@angular/material": "^7.3.3",
		"@angular/platform-browser": "^7.2.7",
		"@angular/platform-browser-dynamic": "^7.2.7",
		"@angular/router": "^7.2.7",
		"@angular/service-worker": "^7.2.7",
		"bootstrap": "^4.3.1",
		"core-js": "^2.6.5",
		"hammerjs": "^2.0.8",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.7",
		"rxjs": "6.4.0",
		"zone.js": "^0.8.29"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.4",
		"@angular/cli": "^7.3.4",
		"@angular/compiler-cli": "^7.2.7",
		"@angular/language-service": "^7.2.7",
		"@types/node": "^11.10.4",
		"codelyzer": "^4.5.0",
		"prettier": "^1.16.4",
		"stylelint": "^9.10.1",
		"stylelint-config-sass-guidelines": "^5.3.0",
		"stylelint-scss": "^3.5.4",
		"ts-node": "^8.0.2",
		"tslint": "^5.13.1",
		"typescript": "3.2.4"
	},
	"engines": {
		"node": "~10.15.0",
		"npm": ">=6.5.0",
		"yarn": "NO LONGER USED - Please use npm"
	}
}
