{
  "name": "devacademy",
  "version": "0.0.1",
  "description": "DevAcademy Website",
  "homepage": "https://devacademy.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/devacademy/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/devacademy.git"
  },
  "scripts": {
    "preinstall": "npm run clean",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags && firebase deploy && git checkout development && git merge origin/master && git push",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -rf node_modules package-lock.json dist .firebase",
    "clean:install": "npm run clean && npm i",
    "lint:fix": "ng lint --fix && stylelint --fix",
    "lint:ts": "ng lint --format stylish",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint": "ng lint devacademy --format stylish",
    "prettier": "prettier --config ./.prettierrc.json --write \"./**/*.{js,ts,json,md,scss,component.html}\"",
    "start": "ng serve -o",
    "deploy": "ng build --prod && firebase deploy"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "rxjs": "6.5.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@types/node": "^12.0.4",
    "codelyzer": "^5.0.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.0.1",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-scss": "^3.6.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "3.4.5"
  },
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.5.0",
    "yarn": "NO LONGER USED - Please use npm"
  }
}
