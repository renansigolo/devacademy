{
	"name": "devacademy",
	"version": "0.0.1",
	"description": "DevAcademy App",
	"homepage": "https://devacademy.com.au",
	"bugs": {
		"url": "https://github.com/renansigolo/devacademy/issues",
		"email": "renan.sigolo@gmail.com"
	},
	"license": "Apache-2.0",
	"author": "Renan Sigolo <renan.sigolo@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/renansigolo/devacademy.git"
	},
	"scripts": {
		"preinstall": "rm -rf node_modules package-lock.json dist",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && firebase deploy && git checkout development && git merge origin/master && git push",
		"build:prod": "ng build --prod",
		"build": "ng build",
		"clean": "rm -r dist node_modules yarn.lock",
		"lint:fix": "ng lint --fix && stylelint --fix",
		"lint:ts": "ng lint --format stylish",
		"lint:scss": "stylelint \"src/**/*.scss\"",
		"lint": "ng lint devacademy --format stylish",
		"prettier": "prettier --config ./.prettierrc.json --write './**/*.{js,jsx,ts,tsx,json,css,scss,md,component.html,yml}'",
		"start": "ng serve -o",
		"deploy": "ng build --prod && firebase deploy",
		"update": "yarn upgrade-interactive --latest"
	},
	"dependencies": {
		"@angular/animations": "^7.1.2",
		"@angular/cdk": "^7.1.1",
		"@angular/common": "^7.1.2",
		"@angular/compiler": "^7.1.2",
		"@angular/core": "^7.1.2",
		"@angular/forms": "^7.1.2",
		"@angular/http": "^7.1.2",
		"@angular/material": "^7.1.1",
		"@angular/platform-browser": "^7.1.2",
		"@angular/platform-browser-dynamic": "^7.1.2",
		"@angular/router": "^7.1.2",
		"@angular/service-worker": "^7.1.2",
		"bootstrap": "^4.1.3",
		"core-js": "^2.6.0",
		"hammerjs": "^2.0.8",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.6",
		"rxjs": "6.3.3",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.11.2",
		"@angular/cli": "^7.1.2",
		"@angular/compiler-cli": "^7.1.2",
		"@angular/language-service": "^7.1.2",
		"@types/node": "^10.12.12",
		"codelyzer": "^4.5.0",
		"prettier": "^1.15.3",
		"stylelint": "^9.9.0",
		"stylelint-config-sass-guidelines": "^5.3.0",
		"stylelint-scss": "^3.4.1",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"typescript": "3.1.6"
	},
	"engines": {
		"node": "^10.14.1",
		"npm": "NO LONGER USED - Please use yarn",
		"yarn": "~1.12.3"
	}
}
