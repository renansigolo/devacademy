{
	"name": "devacademy",
	"version": "0.0.0",
	"description": "DevAcademy App",
	"homepage": "https://devacademy.com.au",
	"bugs": {
		"url": "https://github.com/renansigolo/devacademy/issues",
		"email": "renan.sigolo@gmail.com"
	},
	"license": "Apache-2.0",
	"author": "Renan Sigolo <renan.sigolo@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/renansigolo/devacademy.git"
	},
	"scripts": {
		"preinstall": "rm -rf node_modules package-lock.json dist",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && firebase deploy && git checkout development && git merge origin/master && git push",
		"build": "ng build",
		"build:prod": "ng build --prod",
		"clean": "rm -rf node_modules package-lock.json dist",
		"deploy": "npm run build:prod && firebase deploy",
		"e2e:run": "cypress run",
		"e2e": "cypress open",
		"lint:styles": "stylelint \"src/**/*.scss\"",
		"lint:ts": "ng lint --format stylish",
		"lint": "npm run lint:ts && npm run lint:styles",
		"prettier": "prettier --config ./.prettierrc.json --write './**/*.{js,jsx,ts,tsx,json,css,scss,md,component.html,yml}'",
		"serve": "ng serve -o --host=0.0.0.0",
		"start": "ng serve -o",
		"test": "npm run lint && npm run build:prod"
	},
	"dependencies": {
		"@angular/animations": "^7.1.1",
		"@angular/cdk": "^7.1.0",
		"@angular/common": "~7.1.1",
		"@angular/compiler": "~7.1.1",
		"@angular/core": "~7.1.1",
		"@angular/flex-layout": "^7.0.0-beta.19",
		"@angular/forms": "~7.1.1",
		"@angular/material": "^7.1.0",
		"@angular/platform-browser": "~7.1.1",
		"@angular/platform-browser-dynamic": "~7.1.1",
		"@angular/router": "~7.1.1",
		"core-js": "^2.5.7",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.3.3",
		"tslib": "^1.9.3",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.11.0",
		"@angular/cli": "~7.1.0",
		"@angular/compiler-cli": "~7.1.1",
		"@angular/language-service": "~7.1.1",
		"@types/node": "~10.12.10",
		"codelyzer": "^4.5.0",
		"cypress": "^3.1.2",
		"prettier": "^1.15.2",
		"ts-node": "~7.0.1",
		"tslint": "~5.11.0",
		"tslint-angular": "^1.1.2",
		"tslint-config-prettier": "^1.17.0",
		"typescript": "~3.1.6"
	},
	"engines": {
		"node": "~10.13.0",
		"npm": ">= 6.4.1",
		"yarn": "NO LONGER USED - Please use npm"
	}
}
